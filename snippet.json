"sieve of erasthonas": {
  "prefix": "sieve",
  "body": [
    "int N= 1e7+5;",
    "vector<bool> mark(N+5, true);",
    "vector<int> primes;",
    "",
    "void sieve()",
    "{",
    "    primes.reserve(N+5);",
    "    mark[0] = mark[1] = false;",
    "    for(int i = 2; i<= N; i++)",
    "    {",
    "        if(mark[i]==true)",
    "        {",
    "            for( int j = i*2; j<= N; j+= i) mark[j] = false;",
    "        }",
    "    }",
    "    for(int i = 1; i <= N; i++)",
    "    {",
    "        if(mark[i]==true) primes.push_back(i);",
    "    }",
    "}"
  ],
  "description": ""
